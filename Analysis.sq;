CREATE TABLE IF NOT EXISTS `vehicle_collision`.`vehicle_collision` (
  `ID` INT(11) NOT NULL AUTO_INCREMENT,
  `NUMBER_OF_MOTORIST_INJURED` VARCHAR(45) NULL DEFAULT NULL,
  `CRASH_DATE` VARCHAR(45) NULL DEFAULT NULL,
  `CRASH_TIME` VARCHAR(45) NULL DEFAULT NULL,
  `BOROUGH` VARCHAR(45) NULL DEFAULT NULL,
  `ZIP_CODE` VARCHAR(45) NULL DEFAULT NULL,
  `LATITUDE` VARCHAR(45) NULL DEFAULT NULL,
  `LONGITUDE` VARCHAR(45) NULL DEFAULT NULL,
  `LOCATION` VARCHAR(45) NULL DEFAULT NULL,
  `ON_STREET_NAME` VARCHAR(45) NULL DEFAULT NULL,
  `CROSS_STREET_NAME` VARCHAR(45) NULL DEFAULT NULL,
  `OFF_STREET_NAME` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_PERSONS_INJURED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_PERSONS_KILLED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_PEDESTRIANS_INJURED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_CYCLIST_INJURED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_PEDESTRIANS_KILLED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_CYCLIST KILLED` VARCHAR(45) NULL DEFAULT NULL,
  `NUMBER_OF_MOTORIST_KILLED` VARCHAR(45) NULL DEFAULT NULL,
  `CONTRIBUTING_FACTOR_VEHICLE_1` VARCHAR(45) NULL DEFAULT NULL,
  `CONTRIBUTING_FACTOR_VEHICLE_2` VARCHAR(45) NULL DEFAULT NULL,
  `CONTRIBUTING_FACTOR_VEHICLE_3` VARCHAR(45) NULL DEFAULT NULL,
  `CONTRIBUTING_FACTOR_VEHICLE_4` VARCHAR(45) NULL DEFAULT NULL,
  `CONTRIBUTING_FACTOR_VEHICLE_5` VARCHAR(45) NULL DEFAULT NULL,
  `COLLISION_ID` VARCHAR(45) NULL DEFAULT NULL,
  `VEHICLE_TYPE_CODE_1` VARCHAR(45) NULL DEFAULT NULL,
  `VEHICLE TYPE_CODE_2` VARCHAR(45) NULL DEFAULT NULL,
  `VEHICLE TYPE_CODE_3` VARCHAR(45) NULL DEFAULT NULL,
  `VEHICLE_TYPE_CODE_4` VARCHAR(45) NULL DEFAULT NULL,
  `VEHICLE_TYPE_CODE_5` VARCHAR(45) NULL DEFAULT NULL,
  PRIMARY KEY (`ID`));
  
  SELECT COUNT(*) FROM vehicle_collision;
